{
  "extends": [
    "tslint-eslint-rules", //https://www.npmjs.com/package/tslint-eslint-rules more rules defined
    "tslint-react"
  ],
  "rules": {
       "no-constant-condition": true, //by tslint-eslint-rules
       "no-console": [true, "log", "error"],  //native rules
       "semicolon": [true,"always"], //native rules
       "jsx-no-lambda": true // by tslint-react
  }
}